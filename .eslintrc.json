{
    "extends": ["airbnb-base", "plugin:prettier/recommended"],    
    "plugins": ["prettier"],
    "root": true,
    "rules": { 
        "prettier/prettier": ["error"],
        "camelcase": 2,
        "space-infix-ops": 2,
        "no-console":"off",
        "no-undef": "off",
        "sort-imports": "error",
        "semi": "error",
        "linebreak-style": ["error", "windows"]                   
    },    
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"        
    }

}