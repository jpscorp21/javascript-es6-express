{
    "extends": "airbnb-base",    
    "root": true,
    "rules": {
        "camelcase": 2,
        "space-infix-ops": 2,
        "no-console":"off",
        "no-undef": "off",
        "sort-imports": "error",
        "semi": "error",
        "linebreak-style": ["error", "windows"]                   
    },    
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"        
    }

}